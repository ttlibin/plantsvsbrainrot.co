            if (!gameUrl) {
                updateStatus('‚ùå Êú™ÊâæÂà∞Ê∏∏ÊàèÂπ≥Âè∞: ' + platform);
                return;
            }

            currentGameUrl = gameUrl;
            currentGamePlatform = platform;

            updateStatus(`üöÄ Ê≠£Âú®ÂêØÂä®PWAÂ£≥Ê®°Âºè - ${platform.toUpperCase()}`);

            // ÊòæÁ§∫Âä†ËΩΩÁä∂ÊÄÅ
            const gameSelection = document.getElementById('gameSelection');
            const gameLoading = document.getElementById('gameLoading');
            const gameControls = document.getElementById('gameControls');

            if (gameSelection) gameSelection.style.display = 'none';
            if (gameLoading) gameLoading.style.display = 'flex';
            if (gameControls) gameControls.style.display = 'flex';

            // ÂêØÂä®PWAÂ£≥
            setTimeout(() => {
                launchPWAShell(gameUrl, platform);
            }, 1000);
        }

        function launchPWAShell(gameUrl, platform) {
            const gameFrame = document.getElementById('gameFrame');
            if (!gameFrame) return;

            // ÂàõÂª∫PWA‰ª£ÁêÜÈ°µÈù¢
            const proxyHTML = createPWAProxy(gameUrl, platform);

            try {
                // ‰ΩøÁî®srcdocÂä†ËΩΩ‰ª£ÁêÜÈ°µÈù¢ÔºåÁªïËøáCSPÈôêÂà∂
                gameFrame.srcdoc = proxyHTML;
                gameFrame.style.display = 'block';

                const gameLoading = document.getElementById('gameLoading');
                if (gameLoading) gameLoading.style.display = 'none';

                updateStatus(`‚úÖ PWAÂ£≥Â∑≤ÈÉ®ÁΩ≤ - ${platform.toUpperCase()} Ê∏∏ÊàèÁéØÂ¢É‰∏≠`);

                // ÁõëÂê¨Êù•Ëá™‰ª£ÁêÜÈ°µÈù¢ÁöÑÊ∂àÊÅØ
                window.addEventListener('message', handleProxyMessage);

                // ËÆæÁΩÆË∂ÖÊó∂Ê£ÄÊµã
                setTimeout(() => {
                    if (gameFrame.srcdoc && gameFrame.srcdoc.length > 0) {
                        updateStatus(`üéÆ PWAÂ£≥ËøêË°å‰∏≠ - ${platform.toUpperCase()} Ê∏∏Êàè`);
                    }
                }, 3000);

            } catch (error) {
                console.error('PWAÂ£≥ÂêØÂä®Â§±Ë¥•:', error);
                fallbackToNewWindow(gameUrl, platform);
            }
        }

        function createPWAProxy(gameUrl, platform) {
