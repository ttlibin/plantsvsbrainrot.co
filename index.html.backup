    <style>
        :root {
            --primary-cyber: #00e5ff;
            --secondary-purple: #7c4dff;
            --accent-pink: #ff4081;
            --neon-green: #76ff03;
            --background-dark: #0a0e27;
            --background-medium: #151a3a;
            --background-light: #1e2444;
            --text-primary: #ffffff;
            --text-secondary: #b0b0b0;
            --font-heading: 'Orbitron', monospace;
            --font-body: 'Noto Sans SC', sans-serif;
            --font-mono: 'Space Mono', monospace;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        html {
            scroll-behavior: smooth;
        }

        body {
            font-family: var(--font-body);
            background: linear-gradient(135deg, var(--background-dark) 0%, var(--background-medium) 50%, var(--background-light) 100%);
            color: var(--text-primary);
            min-height: 100vh;
            overflow-x: hidden;
            position: relative;
        }

        /* Animated Background */
        body::before {
            content: '';
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background:
                radial-gradient(circle at 20% 50%, rgba(0, 229, 255, 0.1) 0%, transparent 50%),
                radial-gradient(circle at 80% 80%, rgba(124, 77, 255, 0.1) 0%, transparent 50%),
                radial-gradient(circle at 40% 20%, rgba(255, 64, 129, 0.1) 0%, transparent 50%);
            pointer-events: none;
            z-index: 1;
        }

        .container {
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 2rem;
            position: relative;
            z-index: 2;
        }

        /* Header */
        header {
            padding: 2rem 0;
            text-align: center;
            position: relative;
            z-index: 10;
        }

        .logo {
            font-family: var(--font-heading);
            font-size: 1.5rem;
            font-weight: 900;
            background: linear-gradient(45deg, var(--primary-cyber), var(--accent-pink));
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            text-transform: uppercase;
            letter-spacing: 2px;
            text-shadow: 0 0 10px rgba(0, 229, 255, 0.5);
        }

        @keyframes glow {
            from { filter: drop-shadow(0 0 20px rgba(0, 229, 255, 0.5)); }
            to { filter: drop-shadow(0 0 30px rgba(255, 64, 129, 0.8)); }
        }

        .tagline {
            font-family: var(--font-heading);
            font-size: 1.2rem;
            color: var(--text-secondary);
            text-transform: uppercase;
            letter-spacing: 2px;
            margin-bottom: 2rem;
        }

        /* Navigation */
        .nav {
            background: linear-gradient(135deg, rgba(21, 26, 58, 0.9) 0%, rgba(10, 14, 39, 0.9) 100%);
            backdrop-filter: blur(10px);
            padding: 1.5rem 2rem;
            border: 1px solid rgba(0, 229, 255, 0.3);
            border-radius: 12px;
            margin-bottom: 2rem;
            box-shadow:
                0 8px 32px rgba(0, 229, 255, 0.1),
                inset 0 1px 0 rgba(255, 255, 255, 0.1);
            position: relative;
            overflow: hidden;
            display: flex;
            justify-content: space-between;
            align-items: center;
            flex-wrap: wrap;
            gap: 1rem;
        }

        .nav-links {
            display: flex;
            gap: 2rem;
            list-style: none;
            flex-wrap: wrap;
        }

        .nav-links a {
            color: var(--text-secondary);
            text-decoration: none;
            font-weight: 500;
            font-family: var(--font-mono);
            text-transform: uppercase;
            letter-spacing: 1px;
            transition: all 0.3s ease;
            position: relative;
        }

        .nav-links a::after {
            content: '';
            position: absolute;
            bottom: -5px;
            left: 0;
            width: 0;
            height: 2px;
            background: linear-gradient(90deg, var(--primary-cyber), var(--accent-pink));
            transition: width 0.3s ease;
        }

        .nav-links a:hover {
            color: var(--primary-cyber);
            text-shadow: 0 0 10px rgba(0, 229, 255, 0.8);
        }

        .nav-links a:hover::after {
            width: 100%;
        }

        /* Hero Section */
        .hero {
            padding: 6rem 0;
            text-align: center;
            position: relative;
        }

        .hero h1 {
            font-family: var(--font-heading);
            font-size: var(--text-h1);
            margin-bottom: 1.5rem;
            text-transform: uppercase;
            letter-spacing: 2px;
            line-height: 1.2;
        }

        .hero p {
            font-size: 1.3rem;
            color: var(--text-secondary);
            margin-bottom: 3rem;
            max-width: 800px;
            margin-left: auto;
            margin-right: auto;
            line-height: 1.6;
        }

        .hero-buttons {
            display: flex;
            gap: 1.5rem;
            justify-content: center;
            flex-wrap: wrap;
        }

        /* Game Preview Section */
        .game-preview {
            background: linear-gradient(135deg, var(--background-dark) 0%, #050714 100%);
            border: 2px solid var(--primary-cyber);
            border-radius: 20px;
            padding: 2rem;
            margin: 3rem 0;
            position: relative;
            min-height: 600px;
            display: flex;
            align-items: center;
            justify-content: center;
            text-align: center;
            box-shadow:
                0 20px 60px rgba(0, 229, 255, 0.2),
                inset 0 0 60px rgba(0, 229, 255, 0.05);
            overflow: hidden;
        }

        .game-preview::before {
            content: '';
            position: absolute;
            top: -2px;
            left: -2px;
            right: -2px;
            bottom: -2px;
            background: linear-gradient(45deg, var(--primary-cyber), var(--accent-pink), var(--neon-green), var(--secondary-purple));
            border-radius: 20px;
            z-index: -1;
            animation: border-rotate 4s linear infinite;
        }

        @keyframes border-rotate {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }

        .game-container {
            width: 100%;
            height: 500px;
            position: relative;
            border-radius: 15px;
            overflow: hidden;
        }

        #gameFrame {
            width: 100%;
            height: 100%;
            border: none;
            border-radius: 15px;
            background: #000;
            display: none;
        }

        .game-selection {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            height: 100%;
            background: linear-gradient(135deg, rgba(0, 229, 255, 0.1) 0%, rgba(255, 64, 129, 0.1) 100%);
            border-radius: 15px;
            padding: 2rem;
        }

        .game-title {
            font-family: var(--font-heading);
            font-size: 2.5rem;
            color: var(--primary-cyber);
            margin-bottom: 1rem;
            text-transform: uppercase;
            letter-spacing: 2px;
        }

        .game-subtitle {
            font-size: 1.1rem;
            color: var(--text-secondary);
            margin-bottom: 2rem;
            opacity: 0.9;
        }

        .game-options {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 1.5rem;
            width: 100%;
            max-width: 1000px;
            margin: 0 auto;
        }

        .game-option {
            background: rgba(0, 0, 0, 0.4);
            border: 2px solid transparent;
            border-radius: 15px;
            padding: 2rem;
            text-align: center;
            transition: all 0.3s ease;
            cursor: pointer;
            position: relative;
            overflow: hidden;
        }

        .game-option::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.1), transparent);
            transition: left 0.5s;
        }

        .game-option:hover::before {
            left: 100%;
        }

        .game-option.yandex { border-color: #ffcc00; }
        .game-option.minplay { border-color: #00ff88; }
        .game-option.crazygames { border-color: #ff6b6b; }
        .game-option.roblox { border-color: #00bfff; }

        .game-option:hover {
            transform: translateY(-5px) scale(1.02);
            box-shadow: 0 15px 40px rgba(0, 229, 255, 0.3);
        }

        .game-option h3 {
            font-family: var(--font-heading);
            font-size: 1.5rem;
            margin-bottom: 1rem;
            text-transform: uppercase;
        }

        .game-option p {
            color: var(--text-secondary);
            margin-bottom: 1.5rem;
            font-size: 0.9rem;
        }

        .btn {
            background: linear-gradient(135deg, var(--primary-cyber) 0%, var(--secondary-purple) 100%);
            color: white;
            border: none;
            padding: 1rem 2rem;
            border-radius: 50px;
            font-family: var(--font-heading);
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 1px;
            cursor: pointer;
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
            font-size: 1rem;
            text-decoration: none;
            display: inline-block;
        }

        .btn:hover {
            transform: scale(1.05);
            box-shadow: 0 10px 30px rgba(0, 229, 255, 0.5);
        }

        .btn-play {
            background: linear-gradient(135deg, var(--accent-pink) 0%, var(--neon-green) 100%);
            font-size: 1.1rem;
            padding: 1.2rem 2.5rem;
        }

        .game-loading {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(135deg, rgba(10, 14, 39, 0.95) 0%, rgba(21, 26, 58, 0.95) 100%);
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            z-index: 20;
            backdrop-filter: blur(10px);
            border-radius: 15px;
        }

        .loading-spinner {
            width: 60px;
            height: 60px;
            border: 4px solid rgba(0, 229, 255, 0.3);
            border-top: 4px solid var(--primary-cyber);
            border-radius: 50%;
            animation: spin 1s linear infinite;
            margin-bottom: 1.5rem;
        }

        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }

        .game-controls {
            position: absolute;
            top: 1rem;
            right: 1rem;
            display: none;
            gap: 0.5rem;
            z-index: 15;
        }

        .control-btn {
            background: rgba(0, 229, 255, 0.2);
            border: 1px solid var(--primary-cyber);
            color: var(--primary-cyber);
            width: 40px;
            height: 40px;
            border-radius: 50%;
            cursor: pointer;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.2rem;
            transition: all 0.3s ease;
        }

        .control-btn:hover {
            background: var(--primary-cyber);
            color: var(--background-dark);
            transform: scale(1.1);
        }

        .status-bar {
            background: rgba(0, 0, 0, 0.6);
            border: 1px solid rgba(0, 229, 255, 0.3);
            border-radius: 10px;
            padding: 1rem;
            margin-top: 2rem;
            text-align: center;
            font-family: var(--font-mono);
            color: var(--neon-green);
            backdrop-filter: blur(10px);
        }

        /* Plants Section */
        .plants-section {
            padding: 6rem 0;
            background: linear-gradient(135deg, rgba(0, 255, 136, 0.05) 0%, rgba(0, 229, 255, 0.05) 100%);
            border-radius: 20px;
            margin: 4rem 0;
        }

        .plants-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 2rem;
            margin-top: 3rem;
        }

        .plant-card {
            background: linear-gradient(135deg, rgba(0, 229, 255, 0.1) 0%, rgba(118, 255, 3, 0.1) 100%);
            border: 2px solid rgba(0, 229, 255, 0.3);
            border-radius: 20px;
            padding: 2rem;
            text-align: center;
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
        }

        .plant-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 3px;
            background: linear-gradient(90deg, var(--neon-green), var(--primary-cyber));
            animation: gradient-flow 3s linear infinite;
        }

        @keyframes gradient-flow {
            0% { transform: translateX(-100%); }
            100% { transform: translateX(100%); }
        }

        .plant-card:hover {
            transform: translateY(-10px);
            border-color: var(--neon-green);
            box-shadow: 0 20px 40px rgba(118, 255, 3, 0.3);
        }

        .plant-icon {
            font-size: 4rem;
            margin-bottom: 1rem;
            display: block;
        }

        .plant-card h3 {
            font-family: var(--font-heading);
            font-size: 1.5rem;
            color: var(--neon-green);
            margin-bottom: 1rem;
        }

        .plant-card p {
            color: var(--text-secondary);
            line-height: 1.6;
            margin-bottom: 1rem;
        }

        .plant-stats {
            display: flex;
            justify-content: space-around;
            margin-top: 1.5rem;
            padding-top: 1rem;
            border-top: 1px solid rgba(118, 255, 3, 0.3);
        }

        .stat {
            text-align: center;
        }

        .stat-value {
            font-family: var(--font-heading);
            font-size: 1.2rem;
            color: var(--primary-cyber);
            font-weight: bold;
        }

        .stat-label {
            font-size: 0.8rem;
            color: var(--text-secondary);
            text-transform: uppercase;
        }

        /* Strategy Section */
        .strategy-section {
            padding: 6rem 0;
        }

        .strategy-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
            gap: 2rem;
            margin-top: 3rem;
        }

        .strategy-card {
            background: linear-gradient(135deg, rgba(124, 77, 255, 0.1) 0%, rgba(255, 64, 129, 0.1) 100%);
            border: 2px solid rgba(124, 77, 255, 0.3);
            border-radius: 20px;
            padding: 2rem;
            position: relative;
            overflow: hidden;
            transition: all 0.3s ease;
        }

        .strategy-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 3px;
            background: linear-gradient(90deg, var(--secondary-purple), var(--accent-pink));
            animation: gradient-flow 3s linear infinite;
        }

        .strategy-card:hover {
            transform: translateY(-5px);
            border-color: rgba(255, 64, 129, 0.6);
            box-shadow:
                0 15px 40px rgba(255, 64, 129, 0.2),
                inset 0 0 20px rgba(255, 64, 129, 0.1);
        }

        .strategy-card h4 {
            font-family: var(--font-heading);
            font-size: var(--text-h3);
            color: var(--primary-cyber);
            margin-bottom: 1rem;
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 1px;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .strategy-card ul {
            list-style: none;
            color: var(--text-secondary);
            font-family: var(--font-body);
            line-height: 1.8;
        }

        .strategy-card li {
            margin-bottom: 0.5rem;
            padding-left: 1.5rem;
            position: relative;
        }

        .strategy-card li::before {
            content: '‚ñ∏';
            position: absolute;
            left: 0;
            color: var(--primary-cyber);
            font-weight: bold;
        }

        /* Features Section */
        .features {
            padding: 6rem 0;
        }

        .section-title {
            font-family: var(--font-heading);
            font-size: 3rem;
            text-align: center;
            margin-bottom: 3rem;
            text-transform: uppercase;
            letter-spacing: 2px;
            color: var(--primary-cyber);
        }

        .features-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 2rem;
        }

        .feature-card {
            background: linear-gradient(135deg, rgba(0, 229, 255, 0.1) 0%, rgba(124, 77, 255, 0.1) 100%);
            border: 2px solid rgba(0, 229, 255, 0.3);
            border-radius: 20px;
            padding: 2rem;
            text-align: center;
            transition: all 0.3s ease;
            backdrop-filter: blur(10px);
        }

        .feature-card:hover {
            transform: translateY(-10px);
            border-color: var(--accent-pink);
            box-shadow: 0 20px 40px rgba(255, 64, 129, 0.3);
        }

        .feature-icon {
            font-size: 3rem;
            margin-bottom: 1rem;
            display: block;
        }

        .feature-card h3 {
            font-family: var(--font-heading);
            font-size: 1.5rem;
            margin-bottom: 1rem;
            color: var(--neon-green);
        }

        .feature-card p {
            color: var(--text-secondary);
            line-height: 1.6;
        }

        /* PWA Info Section */
        .pwa-info {
            background: rgba(118, 255, 3, 0.1);
            border: 2px solid rgba(118, 255, 3, 0.3);
            border-radius: 20px;
            padding: 2rem;
            margin-top: 3rem;
            text-align: center;
        }

        .pwa-info h3 {
            font-family: var(--font-heading);
            color: var(--neon-green);
            font-size: 1.8rem;
            margin-bottom: 1rem;
        }

        .pwa-features {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 1rem;
            margin-top: 1.5rem;
        }

        .pwa-feature {
            background: rgba(0, 0, 0, 0.3);
            padding: 1rem;
            border-radius: 10px;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .pwa-feature-icon {
            color: var(--neon-green);
            font-size: 1.2rem;
        }

        /* Game Stats */
        .game-stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
            gap: 1rem;
            margin-top: 2rem;
        }

        .stat-item {
            background: rgba(0, 0, 0, 0.3);
            padding: 1rem;
            border-radius: 10px;
            text-align: center;
        }

        .stat-value {
            font-family: var(--font-heading);
            font-size: 1.5rem;
            color: var(--neon-green);
            font-weight: bold;
        }

        .stat-label {
            font-size: 0.8rem;
            color: var(--text-secondary);
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        /* Footer */
        footer {
            background: var(--background-dark);
            border-top: 2px solid rgba(0, 229, 255, 0.3);
            padding: 3rem 0 2rem;
            margin-top: 6rem;
            text-align: center;
        }

        .footer-content {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 2rem;
            margin-bottom: 2rem;
        }

        .footer-section h4 {
            font-family: var(--font-heading);
            color: var(--primary-cyber);
            margin-bottom: 1rem;
            text-transform: uppercase;
        }

        .footer-section ul {
            list-style: none;
        }

        .footer-section li {
            margin-bottom: 0.5rem;
        }

        .footer-section a {
            color: var(--text-secondary);
            text-decoration: none;
            transition: color 0.3s ease;
        }

        .footer-section a:hover {
            color: var(--accent-pink);
        }

        .copyright {
            border-top: 1px solid rgba(255, 255, 255, 0.1);
            padding-top: 2rem;
            color: var(--text-secondary);
            font-size: 0.9rem;
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            .logo {
                font-size: 1.2rem;
            }

            .nav {
                flex-direction: column;
                text-align: center;
                padding: 1rem;
            }

            .nav-links {
                justify-content: center;
                gap: 1rem;
            }

            .hero h1 {
                font-size: 2.5rem;
            }

            .game-options {
                grid-template-columns: 1fr;
            }

            .features-grid {
                grid-template-columns: 1fr;
            }

            .plants-grid {
                grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            }

            .strategy-grid {
                grid-template-columns: 1fr;
            }

            nav ul {
                flex-direction: column;
                gap: 1rem;
            }
        }

        /* Text Variables */
        :root {
            --text-h1: clamp(2.5rem, 5vw, 4rem);
            --text-h2: clamp(2rem, 4vw, 3rem);
            --text-h3: clamp(1.5rem, 3vw, 2rem);
        }

        /* What Is Section */
        .what-is-section {
            padding: 6rem 0;
            background: linear-gradient(135deg, rgba(0, 229, 255, 0.05) 0%, rgba(255, 64, 129, 0.05) 100%);
            border-radius: 20px;
            margin: 4rem 0;
        }

        .intro-content {
            max-width: 1000px;
            margin: 0 auto;
            text-align: center;
        }

        .intro-content p {
            font-size: 1.2rem;
            color: var(--text-secondary);
            line-height: 1.8;
