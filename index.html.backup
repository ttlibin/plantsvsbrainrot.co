        // Ê®°ÊãüÊ∏∏ÊàèÁªüËÆ°Êï∞ÊçÆÊõ¥Êñ∞
        function updateGameStats() {
            setInterval(() => {
                // ÈöèÊú∫Êõ¥Êñ∞‰∏Ä‰∫õÁªüËÆ°Êï∞ÊçÆ
                const waveEl = document.getElementById('currentWave');
                const brainrotsEl = document.getElementById('collectedBrainrots');
                const incomeEl = document.getElementById('incomePerSecond');
